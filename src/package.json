{
  "name": "valor-registry",
  "version": "1.6.0",
  "description": "A web application for cataloging military medal collections",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:auth": "playwright test tests/auth.spec.ts",
    "test:medals": "playwright test tests/medals.spec.ts",
    "test:admin": "playwright test tests/admin.spec.ts",
    "test:search": "playwright test tests/search.spec.ts",
    "test:contact": "playwright test tests/contact-support.spec.ts",
    "test:settings": "playwright test tests/settings.spec.ts",
    "test:report": "playwright show-report",
    "test:install": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0"
  },
  "keywords": [
    "military",
    "medals",
    "valor-registry",
    "catalog",
    "react",
    "supabase"
  ],
  "author": "Valor Registry Team",
  "license": "MIT"
}